<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Furniture Expanded - Production</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>

				<li Class="PatchOperationAdd">
					<xpath>/Defs/RecipeDef[@Name = "RS_Make_Absteel"]/recipeUsers</xpath>
					<value>
                        <li>VFE_FueledSmelter</li>
					</value>
                </li>

				<li Class="PatchOperationAdd">
					<xpath>/Defs/RecipeDef[@Name = "RS_Make_BlocksAbstone"]/recipeUsers</xpath>
					<value>
                        <li>VFE_TableStonecutterElectric</li>
					</value>
                </li>

				<li Class="PatchOperationAdd">
					<xpath>/Defs/RecipeDef[@Name = "RS_Make_BlocksAbstonefromBlocks"]/recipeUsers</xpath>
					<value>
                        <li>VFE_TableStonecutterElectric</li>
					</value>
                </li>

				<li Class="PatchOperationAdd">
					<xpath>/Defs/RecipeDef[@Name = "RS_Make_Tungsten"]/recipeUsers</xpath>
					<value>
                        <li>VFE_FueledSmelter</li>
					</value>
                </li>

            </operations>
        </match>
    </Operation>

</Patch>