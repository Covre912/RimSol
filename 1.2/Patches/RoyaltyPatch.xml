<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Royalty</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>

				<li Class="PatchOperationAdd">
					<xpath>/Defs</xpath>
					<value>
                        <TerrainDef ParentName="FineTileStoneBase">
                            <defName>RS_FineTileAbstone</defName>
                            <label>fine abstone tile</label>
                            <color>(235,235,235)</color>
                            <costList>
                                <RS_BlocksAbstone>20</RS_BlocksAbstone>
                            </costList>
                        </TerrainDef>
					</value>
                </li>

            </operations>
        </match>
    </Operation>

</Patch>
