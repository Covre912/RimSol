<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <!-- Card Game Table -->
        <ThingDef ParentName="ArtableFurnitureBase">
            <defName>RS_CardGameTable</defName>
            <label>card game table</label>
            <description>.</description> <!-- Adjust -->
            <graphicData>
                <texPath>Things/Building/Joy/CardGameTable/CardGameTable</texPath>
                <graphicClass>Graphic_Multi</graphicClass>
                <shaderType>CutoutComplex</shaderType>
                <drawSize>(1,1)</drawSize>
                <drawRotated>false</drawRotated>
                <allowFlip>false</allowFlip>
                <damageData>
                    <cornerTL>Damage/Corner</cornerTL>
                    <cornerTR>Damage/Corner</cornerTR>
                    <cornerBL>Damage/Corner</cornerBL>
                    <cornerBR>Damage/Corner</cornerBR>
                </damageData>
            </graphicData>
            <altitudeLayer>Building</altitudeLayer>
            <rotatable>false</rotatable>
            <socialPropernessMatters>true</socialPropernessMatters>
            <fillPercent>0.40</fillPercent>
            <minTechLevelToBuild>Industrial</minTechLevelToBuild>
            <building>
                <joyKind>Gaming_Cerebral</joyKind>
            </building>
            <researchPrerequisites>
                <li>ComplexFurniture</li> <!-- Adjust -->
            </researchPrerequisites>
            <statBases>
                <MaxHitPoints>80</MaxHitPoints>
                <WorkToBuild>12000</WorkToBuild>
                <Mass>5</Mass>
                <Flammability>1.0</Flammability>
                <Beauty>4</Beauty>
                <JoyGainFactor>1.3</JoyGainFactor>
            </statBases>
            <stuffCategories>
                <li>Metallic</li>
                <li>Woody</li>
                <li>Stony</li>
            </stuffCategories>
            <costStuffCount>70</costStuffCount>
            <pathCost>30</pathCost>
            <passability>PassThroughOnly</passability>
            <designationCategory>Joy</designationCategory>
        </ThingDef>

</Defs>