<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <!-- Biwa -->
        <ThingDef ParentName="MusicalInstrumentBase">
            <defName>RS_Biwa</defName>
            <label>biwa</label>
            <description>.</description> <!-- Adjust -->
            <size>(1,1)</size>
            <pathCost>30</pathCost>
            <minTechLevelToBuild>Industrial</minTechLevelToBuild>
            <interactionCellOffset>(0,0,-1)</interactionCellOffset>
            <soundPlayInstrument>Harp_Play</soundPlayInstrument> <!-- Adjust -->
            <researchPrerequisites>
                <li>Harp</li> <!-- Adjust -->
            </researchPrerequisites>
            <thingSetMakerTags>
                <li>RewardStandardQualitySuper</li>
            </thingSetMakerTags>
            <statBases>
                <MaxHitPoints>80</MaxHitPoints>
                <WorkToMake>12000</WorkToMake>
                <Mass>4</Mass>
                <JoyGainFactor>1</JoyGainFactor>
                <Beauty>8</Beauty>
            </statBases>
            <recipeMaker>
                <skillRequirements>
                    <Crafting>6</Crafting>
                </skillRequirements>
                <researchPrerequisite>Harp</researchPrerequisite> <!-- Adjust -->
                <recipeUsers>
                    <li>FueledSmithy</li>
                    <li>ElectricSmithy</li>
                </recipeUsers>
            </recipeMaker>
            <costList>
                <WoodLog>85</WoodLog>
            </costList>
            <building>
                <instrumentRange>8</instrumentRange>
            </building>
            <uiIconOffset>(0, 0.08)</uiIconOffset>
            <uiIconScale>0.9</uiIconScale>
            <graphicData>
                <texPath>Things/Building/Joy/Biwa</texPath>
                <drawSize>(1,1)</drawSize>
                <graphicClass>Graphic_Single</graphicClass>
                <drawRotated>false</drawRotated>
                <allowFlip>false</allowFlip>
                <shadowData>
                    <volume>(0.08, 0.2, 0.08)</volume>
                    <offset>(0, 0, -0.1)</offset>
                </shadowData>
                <damageData>
                    <rect>(0.3,0.3,0.4,0.4)</rect>
                </damageData>
            </graphicData>
        </ThingDef>

