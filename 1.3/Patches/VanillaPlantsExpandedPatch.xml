<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Plants Expanded</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>

				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName = "RS_Plant_Watermelon"]</xpath>
					<value>
                        <thingCategories>
                            <li>VCE_Fruit</li>
                        </thingCategories>
					</value>
                </li>

            </operations>
        </match>
    </Operation>

</Patch>